{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information"
        }
    },
    "extensions": {
        "queues": {
            "messageEncoding": "base64",
            "maxPollingInterval": "00:00:02",
            "visibilityTimeout": "00:00:30",
            "batchSize": 16,
            "maxDequeueCount": 5,
            "newBatchThreshold": 8
        }
    }
}